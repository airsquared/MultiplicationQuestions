<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="multiplication.ResultsController">
    <top>
        <Label styleClass="title" text="Results" BorderPane.alignment="CENTER"/>
    </top>
    <center>
        <TableView fx:id="table" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <columns>
                <TableColumn fx:id="date" prefWidth="75.0" resizable="false" text="Date" editable="false"/>
                <TableColumn fx:id="total" prefWidth="75.0" resizable="false" text="Total" editable="false"/>
                <TableColumn fx:id="wrong" prefWidth="75.0" resizable="false" text="Wrong" editable="false"/>
                <TableColumn fx:id="right" prefWidth="75.0" resizable="false" text="Right" editable="false"/>
                <TableColumn fx:id="duration" prefWidth="75.0" resizable="false" text="Duration" editable="false"/>
            </columns>
        </TableView>
    </center>
    <bottom>
        <Button alignment="CENTER" mnemonicParsing="false" text="Back" BorderPane.alignment="BOTTOM_RIGHT"
                onAction="#mainMenu">
            <BorderPane.margin>
                <Insets bottom="10.0" right="10.0"/>
            </BorderPane.margin>
        </Button>
    </bottom>
</BorderPane>
