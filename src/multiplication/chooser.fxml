<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
            prefWidth="600.0" styleClass="pane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="multiplication.ChooserController">
    <top>
        <Label styleClass="title" text="Select a table to practice" BorderPane.alignment="CENTER">
            <BorderPane.margin>
                <Insets top="20.0"/>
            </BorderPane.margin>

        </Label>
    </top>
    <center>
        <GridPane alignment="CENTER" BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <BorderPane.margin>
                <Insets left="20.0" right="20.0" top="30.0"/>
            </BorderPane.margin>
            <children>
                <Button fx:id="onesTable" mnemonicParsing="false" onAction="#tableClickedHandler" prefHeight="27.0"
                        prefWidth="500.0" text="1's Table" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets left="10.0" right="10.0"/>
                    </GridPane.margin>
                </Button>
                <Button mnemonicParsing="false" onAction="#tableClickedHandler" prefHeight="27.0" prefWidth="500.0"
                        text="2 Tables"
                        GridPane.columnIndex="1" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets left="10.0" right="10.0"/>
                    </GridPane.margin>
                </Button>
                <Button mnemonicParsing="false" onAction="#tableClickedHandler" prefHeight="27.0" prefWidth="500.0"
                        text="3 Tables" GridPane.columnIndex="2" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets left="10.0" right="10.0"/>
                    </GridPane.margin>
                </Button>
                <Button mnemonicParsing="false" onAction="#tableClickedHandler" prefHeight="27.0" prefWidth="500.0"
                        text="4 Tables" GridPane.columnIndex="3" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets left="10.0" right="10.0"/>
                    </GridPane.margin>
                </Button>
                <Button mnemonicParsing="false" onAction="#tableClickedHandler" prefHeight="27.0" prefWidth="500.0"
                        text="5 Tables" GridPane.columnIndex="4" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets left="10.0" right="10.0"/>
                    </GridPane.margin>
                </Button>
                <Button mnemonicParsing="false" onAction="#tableClickedHandler" prefHeight="27.0" prefWidth="500.0"
                        text="6 Tables"
                        GridPane.columnIndex="5" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets left="10.0" right="10.0"/>
                    </GridPane.margin>
                </Button>
                <Button mnemonicParsing="false" onAction="#tableClickedHandler" prefHeight="27.0" prefWidth="500.0"
                        text="7 Tables" GridPane.rowIndex="2">
                    <GridPane.margin>
                        <Insets left="10.0" right="10.0"/>
                    </GridPane.margin>
                </Button>
                <Button mnemonicParsing="false" onAction="#tableClickedHandler" prefHeight="27.0" prefWidth="500.0"
                        text="8 Tables" GridPane.columnIndex="1" GridPane.rowIndex="2">
                    <GridPane.margin>
                        <Insets left="10.0" right="10.0"/>
                    </GridPane.margin>
                </Button>
                <Button mnemonicParsing="false" onAction="#tableClickedHandler" prefHeight="27.0" prefWidth="500.0"
                        text="9 Tables" GridPane.columnIndex="2" GridPane.rowIndex="2">
                    <GridPane.margin>
                        <Insets left="10.0" right="10.0"/>
                    </GridPane.margin>
                </Button>
                <Button mnemonicParsing="false" onAction="#tableClickedHandler" prefHeight="27.0" prefWidth="500.0"
                        text="10 Tables" GridPane.columnIndex="3" GridPane.rowIndex="2">
                    <GridPane.margin>
                        <Insets left="10.0" right="10.0"/>
                    </GridPane.margin>
                </Button>
                <Button mnemonicParsing="false" onAction="#tableClickedHandler" prefHeight="27.0" prefWidth="500.0"
                        text="11 Tables" GridPane.columnIndex="4" GridPane.rowIndex="2">
                    <GridPane.margin>
                        <Insets left="10.0" right="10.0"/>
                    </GridPane.margin>
                </Button>
                <Button alignment="CENTER" mnemonicParsing="false" onAction="#tableClickedHandler" prefHeight="27.0"
                        prefWidth="273.0" text="12 Tables" GridPane.columnIndex="5" GridPane.rowIndex="2">
                    <GridPane.margin>
                        <Insets left="10.0" right="10.0"/>
                    </GridPane.margin>
                </Button>
                <Button mnemonicParsing="false" onAction="#tableClickedHandler" prefHeight="27.0" prefWidth="500.0"
                        text="All(1-12)" GridPane.columnIndex="5">
                    <GridPane.margin>
                        <Insets left="10.0" right="10.0" top="40.0"/>
                    </GridPane.margin>
                </Button>
                <TextField fx:id="numQuestions" alignment="BOTTOM_LEFT" text="10" GridPane.columnSpan="2">
                    <GridPane.margin>
                        <Insets left="10.0" top="50.0"/>
                    </GridPane.margin>
                </TextField>
                <Label alignment="TOP_LEFT" text="How many questions?" textOverrun="CLIP" GridPane.columnSpan="2">
                    <GridPane.margin>
                        <Insets left="10.0"/>
                    </GridPane.margin>
                </Label>
                <Button mnemonicParsing="false" prefHeight="27.0" prefWidth="500.0" text="Previous Results"
                        onAction="#prevResults" fx:id="prevResults" GridPane.columnIndex="3">
                    <GridPane.margin>
                        <Insets left="10.0" right="10.0" top="40.0"/>
                    </GridPane.margin>
                </Button>
            </children>
        </GridPane>
    </center>
</BorderPane>
